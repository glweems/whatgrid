{
  "name": "whatgrid-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,js,json,yml,md}\"",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:fix": "npm run lint:es --fix && npm run lint:style",
    "lint:es": "eslint --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "lint:style": "stylelint --ignore-path .gitignore \"src/**/*.{ts,tsx,js}\"",
    "serve": "gatsby serve",
    "test": "jest",
    "type-check": "tsc",
    "gql-json": "yarn run apollo schema:download --endpoint=http://localhost:4000/graphql graphql-schema.json",
    "gql-ts": "yarn run apollo codegen:generate --localSchemaFile=graphql-schema.json --target=typescript --includes=src/graphql/**/*.ts --tagName=gql --addTypename --globalTypesFile=src/graphql-global-types.ts types",
    "get-types": "yarn gql-json && yarn gql-ts",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true,
      "ssr": true
    }
  },
  "dependencies": {
    "@apollo/federation": "^0.11.2",
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hoc": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@rhysforyou/gatsby-plugin-react-helmet-async": "^0.1.1",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/styled-components": "4.1.8",
    "@types/uuid": "^3.4.6",
    "@types/yup": "^0.26.26",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "babel-jest": "^24.9.0",
    "babel-plugin-console": "^0.2.1",
    "csstype": "^2.6.7",
    "dotenv": "^8.2.0",
    "easy-peasy": "^3.2.3",
    "emotion-theming": "^10.0.19",
    "formik": "^2.0.6",
    "gatsby": "^2.18.5",
    "gatsby-plugin-apollo": "^0.1.1",
    "gatsby-plugin-manifest": "^2.2.30",
    "gatsby-plugin-styled-components": "^3.1.14",
    "gatsby-plugin-typescript": "^2.1.20",
    "gitmoji-cli": "^3.0.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "isomorphic-fetch": "^2.2.1",
    "jest-styled-components-stylelint": "^0.7.0",
    "lodash": "^4.17.15",
    "polished": "^3.4.2",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dnd": "^9.5.1",
    "react-dnd-html5-backend": "^9.5.1",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "react-spring": "^8.0.27",
    "react-testing-library": "^8.0.1",
    "reinspect": "^1.0.2",
    "styled-components": "^5.0.0-rc.2",
    "uuid": "^3.3.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.9.1",
    "@graphql-codegen/introspection": "^1.9.1",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.9.1",
    "@graphql-codegen/typescript-operations": "^1.9.1",
    "@graphql-codegen/typescript-react-apollo": "^1.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.13",
    "@types/react-dnd": "^3.0.2",
    "@types/react-helmet": "^5.0.14",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "apollo": "^2.21.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-plugin-graphql-tag": "^2.5.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-gatsby": "^0.2.23",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.25",
    "lint-staged": "^9.5.0",
    "react-test-renderer": "^16.12.0"
  }
}
