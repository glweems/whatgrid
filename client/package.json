{
  "name": "client",
  "root": true,
  "version": "0.0.1",
  "license": "MIT",
  "repository": "git@github.com:glweems/whatgrid.git",
  "scripts": {
    "dev": "yarn run clean && yarn run generate && yarn run next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "clean": "rm -rf ./next && echo .next dir cleared && rm -rf out && echo out dir cleared",
    "deploy": "next build && next export",
    "format": "prettier --ignore-path .gitignore --write -c .prettierrc \"./**/*.{ts,tsx,js,json,yml,md}\"",
    "lint": "yarn lint:es && yarn lint:style",
    "lint:es": "eslint -c .eslintrc.js --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "lint:fix": "yarn format && yarn lint:es --fix && yarn lint:style",
    "lint:style": "stylelint --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,tsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix",
      "pre-push": "yarn build"
    }
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true,
      "ssr": true,
      "displayName": true
    }
  },
  "dependencies": {
