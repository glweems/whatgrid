    "js-cookie": "^2.2.1",
    "lodash": "^4.17.15",
    "next": "9.2",
    "next-with-apollo": "^4.3.0",
    "node-fetch": "^2.6.0",
    "polished": "^3.4.2",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "16.12.0",
    "react-modal": "^3.11.1",
