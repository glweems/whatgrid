{
  "name": "client",
  "root": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run format generate && next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "next build && next export",
    "format": "prettier --ignore-path .gitignore --write -c .prettierrc \"./**/*.{ts,tsx,js,json,yml,md}\"",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint -c .eslintrc.js --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "lint:fix": "npm run format && npm run lint:es --fix && npm run lint:style",
    "lint:style": "stylelint --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix",
      "pre-push": "yarn build"
    }
  },
  "babelMacros": {
    "styledComponents": {
      "pure": true,
      "ssr": true,
      "displayName": true
    }
  },
